&lt;audio controls style=&#34;width: 100%; margin-bottom: 20px&#34;&gt;
    
    &lt;source src=&#34;../../audio/bach/pairs/pair_3/bach_43.mp3&#34; type=&#34;audio/mpeg&#34;&gt;
    
