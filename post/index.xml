&lt;audio controls style=&#34;width: 100%; margin-bottom: 20px&#34;&gt;
    
    &lt;source src=&#34;../../audio/bach/pairs/pair_2/base_13.mp3&#34; type=&#34;audio/mpeg&#34;&gt;
    
